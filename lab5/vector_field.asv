function vector_field( A, u1ma, u1min, u2max, u2min)
    u1 = linspace(u1min, u1max, 30); 
    u2 = linspace(u2min, u2max, 30);

    [U1, U2] = meshgrid(u1, u2);
    F1 = A(1,1) * U1 + A(1,2) * U2; 
    F2 = A(2,1) * U1 + A(2,2) * U2;
    quiver(U1,U2,F1,F2,0.9);
    axis([u1min u1max u2min max]), hold on


end

